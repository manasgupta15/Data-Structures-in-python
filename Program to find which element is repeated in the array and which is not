def prinntrep(array,size):
    print("The repeated element in the array is: ",end=" ")
    for i in range(0,size-1):
        for j in range(i+1,size):
            if(array[i]==array[j]):
                print(array[i],end=" ")
                
                
array=[1,2,3,4,5,6,2,3,4,1]
size=len(array)
result=prinntrep(array,size)
print(result)
